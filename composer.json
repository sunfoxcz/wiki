{
    "name": "sunfoxcz/foxywiki",
    "description": "Simple database-less wiki built using Nette framework",
    "type": "project",
    "license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
    "authors": [
        {
            "name": "Tomas Jacik",
            "email": "tomas.jacik@sunfox.cz",
            "homepage": "https://sunfox.cz"
        }
    ],
    "require": {
        "php": ">=7.2 <8.1",
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.1",
        "nette/caching": "^3.1",
        "nette/di": "^3.0",
        "nette/forms": "^3.1",
        "nette/http": "^3.1",
        "nette/security": "^3.1",
        "nette/utils": "^3.2",
        "latte/latte": "^2.9",
        "tracy/tracy": "^2.8",
        "nextras/forms-rendering": "^1.1",
        "contributte/menu-control": "^2.2",
        "league/commonmark": "^2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "sunfoxcz/coding-standard": "dev-master",
        "nette/tester": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "ocramius/package-versions": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
